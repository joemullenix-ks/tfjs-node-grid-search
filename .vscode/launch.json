{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch JS",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}\\src\\all.js"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch TS Output",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}\\built\\all.js"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch TypeScript Sandbox",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}\\built\\ts_sandbox.js"
		}
	]
}