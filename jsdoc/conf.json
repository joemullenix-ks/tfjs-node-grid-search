{
    "plugins": [],
    "recurseDepth": 100,
    "source": {
        "include": ["dist\\esm"],
        "includePattern": ".+\\.js(doc|x)?$"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false,
        "default":{
            "includeDate": false
        }
    },
    "opts": {
        "destination": "docs",
        "recurse": true
    }
}